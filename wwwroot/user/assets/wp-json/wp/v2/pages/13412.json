r\u00ecnh b\u00e0y, bi\u1ec3u di\u1ec5n, ho\u1eb7c t\u1ed5 ch\u1ee9c s\u1ef1 ki\u1ec7n trong ph\u00f2ng h\u1ecdc.<\/p>\n\t\t<\/div>\n\t<\/div>\n\t<\/a>\n\t\n\t\t<\/div>\n\t\t\t\t\n<style>\n#col-2011256109 > .col-inner {\n  padding: 10px 5px 20px 10px;\n  border-radius: 5px;\n}\n<\/style>\n\t<\/div>\n\n\t\n<\/div>\n\t\t<\/div>\n\n\t\t\n<style>\n#section_2020015532 {\n  padding-top: 30px;\n  padding-bottom: 30px;\n}\n<\/style>\n\t<\/section>\n\t\n\n","protected":false},"excerpt":{"rendered":"","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"page-blank.php","meta":{"_acf_changed":false,"inline_featured_image":false,"footnotes":""},"class_list":["post-13412","page","type-page","status-publish","hentry"],"acf":[],"_links":{"self":[{"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/pages\/13412","targetHints":{"allow":["GET"]}}],"collection":[{"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/pages"}],"about":[{"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/types\/page"}],"author":[{"embeddable":true,"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/comments?post=13412"}],"version-history":[{"count":14,"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/pages\/13412\/revisions"}],"predecessor-version":[{"id":15021,"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/pages\/13412\/revisions\/15021"}],"wp:attachment":[{"href":"https:\/\/inno.com.vn\/wp-json\/wp\/v2\/media?parent=13412"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}